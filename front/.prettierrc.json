{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "endOfLine": "lf",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],

  "importOrder": [
    "^react$",
    "^react(.*)$",
    "^@mui/(.*)$",
    "^shared(.*)$",
    "<THIRD_PARTY_MODULES>",
    "(.*)scss$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
